    c.acc_out_id,
  CASE
   WHEN c.trn_code = 'E060' AND c.opr_code = 'DLST' THEN
   CASE
    WHEN s_1.sec_type_id = '3' THEN 'Y'
    ELSE 'N'
   END
   ELSE c.tax_applicable
  END AS tax_applicable,
    c.trn_code,
